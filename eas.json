{
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "env": {
          "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk-amd64"
        }
      }
    }
  },
  "cli": {
    "version": ">=0.64.0"
  }
}
